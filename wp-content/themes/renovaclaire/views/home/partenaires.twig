<div class="partenaires">
	<h2 class="petit-titre">Certifications</h2>
	<article>
		<header>
			{{home.certifications_presentation}}
		</header>
		<div>
			<ul>

				{% for certification in home.certifications %}
					<li>
						<a target="_blank" href="{{certification.url}}" title="{{certification.url}}"><img src="{{certification.logo.sizes.medium}}" alt="{{certification.url}}"></a>
					</li>
				{% endfor %}
			</ul>
		</div>
	</article>
</div>
